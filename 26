class Solution(object):
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        for i in range(len(nums)-1, 0, -1):   #顺序遍历会遇到index溢出的情况，于是采用逆序遍历
            if nums[i] == nums[i-1]: nums.pop(i)
        return len(nums)
if __name__ == "__main__":
    test = Solution()
    t =  test.removeDuplicates([1,1,2])
    print(t)
