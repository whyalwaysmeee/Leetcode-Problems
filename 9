class Solution(object):
    def isPalindrome(self, x):
        """
        :type x: int
        :rtype: bool
        """
        if x < 0:
            return 0
        else:
            r = str(x)
            if r == r[::-1]:
                return 1
            else:
                return 0
if __name__ == "__main__":
    test  = Solution()
    answer = test.isPalindrome(81273)
    if answer == 1:
        print('true')
    else:
        print('false')
