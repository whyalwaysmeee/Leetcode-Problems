class Solution(object):
    def romanToInt(self,s):
        t = 0
        dic = {'I': 1, 'V': 5, 'X': 10, 'L': 50, 'C': 100, 'D': 500, 'M': 1000}
        for i in range(len(s)):
                if i < len(s) -1 and dic[s[i]] < dic[s[i + 1]]:
                    t = t - dic[s[i]]
                else:
                    t = t + dic[s[i]]
        return t 

a = Solution()
a.romanToInt('LVIII')
