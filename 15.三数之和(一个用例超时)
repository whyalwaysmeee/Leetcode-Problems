class Solution(object):
    def threeSum(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        nums = sorted(nums)
        res = []
        m = len(nums)
        for i in range(m-2):
            for j in range(i+1,m-1):
                p = []
                if (0-nums[i]-nums[j]) in nums[j+1:]:
                    p.append(nums[i])
                    p.append(nums[j])
                    p.append(0-nums[i]-nums[j])
                if p != [] and p not in res:
                    res.append(p)
        return res
